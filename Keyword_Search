def keyword_search(list_a, keyword):
  for item in list_a:
    if type(item) == int:
      print("This is an integer.")
    if type(item) == float:
      print("This is a float.")
    if type(item) == str:
      print("This is a string.")
      if item == keyword:
        print("You found the keyword!")
    
    
    
